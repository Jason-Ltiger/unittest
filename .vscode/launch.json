{
    "version": "0.2.0",
    "configurations": [
    
      {
        "name": "launch debug",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/unittest",
        "args": ["--simu_mode"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "linux":{
          "MIMode": "gdb",
          "setupCommands": [
            {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
            }
          ]
        },
        
        "miDebuggerPath": "/usr/bin/gdb",
        "logging": {
            "engineLogging": false
        },
        
        "preLaunchTask": "makebuild",
      }
    ]
  }